    </field>
    <field Name="customS1" Description="定制字符串" Index="" Format="" Value="" /> 
    <field Name="customS2" Description="定制字符串" Index="" Format="" Value="" />
    <field Name="customS3" Description="定制字符串" Index="" Format="" Value="" />
