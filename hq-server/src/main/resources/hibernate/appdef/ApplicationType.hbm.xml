<hibernate-mapping package="org.hyperic.hq.appdef.server.session">
  <class name="ApplicationType" table="EAM_APPLICATION_TYPE">

    <id name="id" type="integer">
      <column name="ID" not-null="true"/>
      <generator class="org.hyperic.hibernate.id.ComboGenerator">
        <param name="sequence">EAM_APPLICATION_TYPE_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="name" type="string">
      <column name="NAME" not-null="true" length="200"/>
    </property>

    <property name="sortName" type="string">
      <column name="SORT_NAME" not-null="false" length="200"/>
    </property>

    <property name="cid" type="integer">
      <column name="CID" not-null="false"/>
    </property>

    <property name="description" type="string">
      <column name="DESCRIPTION" not-null="false" length="200"/>
    </property>

    <property name="creationTime" type="long">
      <column name="CTIME" not-null="false"/>
    </property>

    <property name="modifiedTime" type="long">
      <column name="MTIME" not-null="false"/>
    </property>

    <bag inverse="true" name="serviceTypes" table="EAM_APP_TYPE_SERVICE_TYPE_MAP">
      <key column="APPLICATION_TYPE_ID" not-null="false"/>
      <many-to-many class="ServiceType" column="SERVICE_TYPE_ID"/>
    </bag>

    <bag inverse="true" cascade="save-update,delete,evict,persist,merge" name="applications" table="EAM_APPLICATION">
      <key on-delete="cascade" column="APPLICATION_TYPE_ID"/>
      <one-to-many class="Application"/>
    </bag>

  </class>

</hibernate-mapping>
