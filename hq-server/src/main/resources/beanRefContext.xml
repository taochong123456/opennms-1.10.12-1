<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" default-lazy-init="true">
        
  <bean id="hqserver" class="org.springframework.context.support.ClassPathXmlApplicationContext">
     <constructor-arg>
       <list>
         <value>META-INF/spring/app-context.xml</value>
       </list>
     </constructor-arg>
     <constructor-arg ref="daoContext" />
   </bean>
   
</beans>
