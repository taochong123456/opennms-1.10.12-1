<hibernate-mapping package="org.hyperic.hq.events.server.session">

  <class name="RegisteredTrigger" table="EAM_REGISTERED_TRIGGER">
    <cache usage="read-write"/>
    <id name="id" type="integer">
      <column name="ID" not-null="true"/>
      <generator class="org.hyperic.hibernate.id.ComboGenerator">
        <param name="sequence">EAM_REGISTERED_TRIGGER_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="frequency" type="long">
      <column name="FREQUENCY" not-null="true" default="0"/>
    </property>

    <property name="classname" type="string">
      <column name="CLASSNAME" not-null="true" length="200"/>
    </property>

    <property lazy="true" name="config" type="binary">
      <column name="CONFIG"/>
    </property>

    <many-to-one name="alertDefinition" class="AlertDefinition">
      <column name="ALERT_DEFINITION_ID" index="ALERT_DEF_TRIGGER_IDX"/>
    </many-to-one>
  </class>

</hibernate-mapping>
